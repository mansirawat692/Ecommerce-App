<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".seller.sellerhomepage"
   >
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:id="@+id/rl6">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="Approved Products"
        android:gravity="center"
        android:textColor="#FFFFFF"
        android:id="@+id/txtvi"
        android:background="#2BE9F3"
        android:textSize="20dp"
        android:textStyle="bold"></TextView>



</RelativeLayout>



<androidx.recyclerview.widget.RecyclerView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_below="@+id/rl6"
    android:layout_marginTop="10dp"
    android:id="@+id/rv6"></androidx.recyclerview.widget.RecyclerView>

<com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:background="?android:attr/windowBackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
    app:menu="@menu/bottomnav" ></com.google.android.material.bottomnavigation.BottomNavigationView>





</RelativeLayout>